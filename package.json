{
  "name": "fsxx",
  "type": "module",
  "version": "0.0.5",
  "packageManager": "pnpm@7.1.7",
  "description": "File system in zx style",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/fsxx#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/fsxx.git"
  },
  "bugs": {
    "url": "https://github.com/antfu/fsxx/issues"
  },
  "keywords": [
    "zx",
    "fs"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs"
    }
  },
  "main": "index.mjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "files": [
    "*.mjs",
    "*.js",
    "*.d.ts"
  ],
  "engines": {
    "node": ">= 14.8.0"
  },
  "scripts": {
    "release": "bumpp --commit --push --tag",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "node test/index.test.mjs"
  },
  "dependencies": {
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "zx": "^6.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.0",
    "@antfu/ni": "^0.16.2",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.36",
    "bumpp": "^7.1.1",
    "esbuild-node-loader": "^0.8.0",
    "eslint": "^8.16.0",
    "tasuku": "^2.0.0",
    "typescript": "^4.7.2"
  }
}
