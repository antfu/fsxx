{
  "name": "fsxx",
  "version": "0.0.3",
  "description": "File system in zx style",
  "keywords": [
    "zx",
    "fs"
  ],
  "engines": {
    "node": ">= 14.8.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/fsxx.git"
  },
  "funding": "https://github.com/sponsors/antfu",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "type": "module",
  "files": [
    "*.mjs",
    "*.js",
    "*.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./index.mjs"
    }
  },
  "main": "index.mjs",
  "module": "index.mjs",
  "types": "index.d.ts",
  "bugs": {
    "url": "https://github.com/antfu/fsxx/issues"
  },
  "homepage": "https://github.com/antfu/fsxx#readme",
  "scripts": {
    "release": "bumpp --commit --push --tag",
