    "test": "node test/index.test.mjs"
  },
  "dependencies": {
    "fast-glob": "^3.2.5",
    "fs-extra": "^10.0.0",
    "zx": "^1.14.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.6.2",
    "@antfu/ni": "^0.5.5",
    "@types/fs-extra": "^9.0.11",
    "@types/node": "^14.14.37",
    "bumpp": "^6.0.6",
    "esbuild-node-loader": "^0.0.0",
    "eslint": "^7.23.0",
    "tasuku": "^1.0.0",
    "typescript": "^4.2.3"
  }
}
